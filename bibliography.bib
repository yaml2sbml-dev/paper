	Keywords = {Cetuximab; EGFR; Gastric cancer; Invasion; Motility},
	Mesh = {Antineoplastic Agents, Immunological; Cell Line, Tumor; Cell Movement; Cetuximab; Humans; Neoplasm Invasiveness; Phenotype; Phosphorylation; Proteome; Receptor, Epidermal Growth Factor; Signal Transduction; Stomach Neoplasms},
	Month = {12},
