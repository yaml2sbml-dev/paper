	Keywords = {axolotl; cell proliferation; computational biology; developmental biology; modeling; regeneration; stem cells; systems biology},
	Mesh = {Ambystoma mexicanum; Animals; Cell Proliferation; Models, Theoretical; Neural Stem Cells; Spatio-Temporal Analysis; Spinal Cord Regeneration},
	Month = {Nov},
	Pmc = {PMC5182066},
