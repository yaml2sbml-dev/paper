	Mesh = {Antineoplastic Agents; Cell Line, Tumor; Drug Screening Assays, Antitumor; Humans; Spheroids, Cellular},
	Number = {3},
	Pages = {309-24},
	Pmid = {19214182},
	Pst = {ppublish},
	Title = {Spheroid-based drug screen: considerations and practical approach},
	Volume = {4},
