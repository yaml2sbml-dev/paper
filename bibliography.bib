	Doi = {10.1101/519579},
	Elocation-Id = {519579},
	Eprint = {https://www.biorxiv.org/content/early/2019/01/13/519579.full.pdf},
	Journal = {bioRxiv},
	Publisher = {Cold Spring Harbor Laboratory},
	Title = {Memory-driven computing accelerates genomic data processing},
	Url = {https://www.biorxiv.org/content/early/2019/01/13/519579},
	Year = {2019},
	Bdsk-Url-1 = {https://www.biorxiv.org/content/early/2019/01/13/519579},
