	Db = {PMC},
	Doi = {10.1371/journal.pcbi.1002644},
	Editor = {Teichmann, Sarah A},
	Isbn = {1553-734X; 1553-7358},
	J1 = {PLoS Comput Biol},
	Journal = {{PLoS} Comput. Biol.},
	Month = {08},
	Number = {8},
	Pages = {e1002644},
	Publisher = {Public Library of Science},
	Title = {Teasing Apart Translational and Transcriptional Components of Stochastic Variations in Eukaryotic Gene Expression},
	Url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC3431295/},
	Volume = {8},
	Year = {2012},
