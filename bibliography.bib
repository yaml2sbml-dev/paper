	Doi = {10.1186/s12918-015-0174-y},
	Journal = {BMC Syst. Biol.},
	Journal-Full = {BMC systems biology},
	Mesh = {Carbon; Database Management Systems; Internet; Models, Biological; Sulfolobus; Systems Biology; User-Computer Interface},
	Month = {Jul},
	Pages = {33},
