	Keywords = {Computational biology; In silico model; Mathematical model; Regeneration; Simulation; Systems biology},
	Mesh = {Animals; Body Patterning; Cell Differentiation; Cell Movement; Computational Biology; Humans; Models, Biological; Regeneration; Systems Biology},
	Pages = {283--317},
